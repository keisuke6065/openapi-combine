{
  "name": "openapi-combine",
  "version": "0.3.1",
  "author": "keisuke.nakamura @keisuke6065",
  "bin": {
    "openapi-combine": "./bin/run"
  },
  "bugs": "https://github.com/keisuke6065/openapi-combine/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "^17.0.41",
    "cli-ux": "^6.0.9",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mkdirp": "^1.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@types/chai": "^4.3.1",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.1",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.26.0",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/keisuke6065/openapi-combine",
  "keywords": [
    "oclif",
    "openapi",
    "openapi3",
    "swagger",
    "combine",
    "merge"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "openapi-combine",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "keisuke6065/openapi-combine",
  "scripts": {
    "start": "./bin/run",
    "build": "tsc -b",
    "lint": "eslint -c .eslintrc 'src/**/*.ts'",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "readme": "oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
