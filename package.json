{
  "name": "openapi-combine",
  "version": "0.1.0",
  "author": "keisuke.nakamura @keisuke6065",
  "bin": {
    "openapi-combine": "./bin/run"
  },
  "bugs": "https://github.com/keisuke6065/openapi-combine/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^14.14.8",
    "cli-ux": "^5.4.10",
    "js-yaml": "^3.14.0",
    "json-refs": "^3.0.15",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "mkdirp": "^1.0.4",
    "tslib": "^1",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/chai": "^4.2.12",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "chai": "^4.2.0",
    "eslint": "^7.7.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "ts-node": "^9.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/keisuke6065/openapi-combine",
  "keywords": [
    "oclif",
    "openapi",
    "openapi3",
    "swagger",
    "combine",
    "merge"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "openapi-combine",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "keisuke6065/openapi-combine",
  "scripts": {
    "start": "./bin/run",
    "lint": "eslint -c .eslintrc 'src/**/*.ts'",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "readme": "oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
